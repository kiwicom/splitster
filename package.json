{
  "name": "@kiwicom/splitster",
  "version": "0.0.56",
  "description": "Javascript AB testing tool",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "babel src -d lib --ignore **/*.test.js",
    "build:watch": "babel src -d lib --ignore **/*.test.js --watch",
    "test": "ALL_TESTS=true jest",
    "lint": "eslint --ext js,js.flow src --quiet",
    "lint:fix": "yarn lint --fix",
    "flow": "flow",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "localsync": "rimraf lib && yarn build && yalc push"
  },
  "repository": "git+https://github.com/kiwicom/splitster.git",
  "author": "VladimÃ­r Jarabica",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kiwicom/splitster/issues"
  },
  "homepage": "https://github.com/kiwicom/splitster#readme",
  "files": [
    "lib/**",
    "src/**/*.js.flow",
    ".flowconfig"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@kiwicom/eslint-config": "^9.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-ramda": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "eslint": "^7.13.0",
    "express": "^4.15.2",
    "flow-bin": "^0.87.0",
    "immutable-devtools": "0.0.7",
    "jest": "^26.6.3",
    "js-cookie": "2.2.1",
    "ramda": "0.27.1",
    "rimraf": "^2.6.3",
    "webpack": "^3.0.0"
  },
  "peerDependencies": {
    "js-cookie": ">= 2.1.4",
    "ramda": ">= 0.26.1"
  },
  "dependencies": {
    "random-js": "^2.1.0",
    "seedrandom": "^3.0.5"
  }
}
